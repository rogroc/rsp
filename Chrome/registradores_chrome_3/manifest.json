{
  "manifest_version": 3,
  "name": "Consulta NIF Registradors",
  "version": "1.0",
  "description": "Extensió per consultar dades d'empresa per NIF al portal OpenData del Col·legi de Registradors.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://opendata.registradores.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}